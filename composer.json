{
    "require": {
        "carlcs/craft-redactorcustomstyles": "^3.0",
        "craftcms/cms": "3.6.17",
        "craftcms/contact-form": "^2.2",
        "craftcms/redactor": "2.8.7",
        "doublesecretagency/craft-cpcss": "^2.2",
        "mmikkel/reasons": "^2.1",
        "nystudio107/craft-imageoptimize": "1.6.34",
        "nystudio107/craft-seomatic": "3.3.44",
        "putyourlightson/craft-blitz": "^3.6",
        "putyourlightson/craft-blitz-recommendations": "^1.1",
        "spicyweb/craft-neo": "2.10.7",
        "studioespresso/craft-seeder": "^3.2",
        "topshelfcraft/wordsmith": "^3.3",
        "vaersaagod/dospaces": "^1.1",
        "verbb/field-manager": "2.2.4",
        "verbb/formie": "1.4.9",
        "verbb/image-resizer": "^2.0",
        "verbb/navigation": "1.4.19",
        "verbb/super-table": "2.6.8",
        "vlucas/phpdotenv": "^3.4.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-yii2": "^1.1",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.5"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
